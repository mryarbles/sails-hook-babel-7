{
  "name": "sails-hook-babel",
  "version": "0.0.1",
  "description": "Sails JS hook to activate babel 7 for the whole api folder",
  "main": "index.js",
  "scripts": {
    "test": "node tests/runner",
    "test-all": "node tests/runner all",
    "autotest": "mocha --watch --reporter spec tests/**/*Test.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mryarbles/sails-hook-babel-7.git"
  },
  "keywords": [
    "sails",
    "hook",
    "babel",
    "transpiler",
    "ES6",
    "ES2015",
    "ES7",
    "sails-hook"
  ],
  "author": "Markus Padourek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mryarbles/sails-hook-babel-7/issues"
  },
  "homepage": "https://github.com/mryarbles/sails-hook-babel-7",
  "sails": {
    "isHook": true
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/register": "7.0.0",
    "source-map-support": "0.5.9"
  },
  "devDependencies": {
    "glob": "^5.0.3",
    "mocha": "^2.2.1",
    "sails": "~0.11.0"
  },
  "contributors": [
    {
      "name": "Markus Padourek"
    },
    {
      "name": "Luke Grannis"
    }
  ]
}
